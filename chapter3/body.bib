% weight selection schemes
@article{roy1996_moo_weights,
author = {Roy, Bernard and Mousseau, Vincent},
title = {A Theoretical Framework for Analysing the Notion of Relative Importance of Criteria},
journal = {Journal of Multi-Criteria Decision Analysis},
volume = {5},
number = {2},
pages = {145-159},
keywords = {decision aid, importance of criteria, weights, aggregation procedures},
doi = {10.1002/(SICI)1099-1360(199606)5:2<145::AID-MCDA99>3.0.CO;2-5},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/\%28SICI\%291099-1360\%28199606\%295\%3A2\%3C145\%3A\%3AAID-MCDA99\%3E3.0.CO\%3B2-5},
abstract = {Abstract Multiple-criteria decision aid almost always requires the use of weights, importance coefficients or even a hierarchy of criteria, veto thresholds, etc. These are importance parameters that are used to differentiate the role devoted to each criterion in the construction of comprehensive preferences. Many researchers have studied the problem of how to assign values to such parameters, but few of them have tried to analyse in detail what underlies the notion of importance of criteria and to give a clear formal definition of it. In this paper our purpose is to define a theoretical framework so as to analyse the notion of the importance of criteria under very general conditions. Within this framework it clearly appears that the importance of criteria is taken into account in very different ways in various aggregation procedures. This framework also allows us to shed new light on fundamental questions such as: Under what conditions is it possible to state that one criterion is more important than another? Are importance parameters of the various aggregation procedures dependent on or independent of the encoding of criteria? What are the links between the two concepts of the importance of criteria and the compensatoriness of preferences? This theoretical framework seems to us sufficiently general to ground further research in order to define theoretically valid elicitation methods for importance parameters.},
year = {1996}
}

@article{podinovski2002_moo_weights,
author = {Podinovski, Vladislav V.},
title = {The quantitative importance of criteria For {MCDA}},
journal = {Journal of Multi-Criteria Decision Analysis},
volume = {11},
number = {1},
pages = {1-15},
keywords = {multiple criteria, criteria importance, coefficients of importance, preference relations},
doi = {10.1002/mcda.312},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mcda.312},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/mcda.312},
abstract = {Abstract This paper presents theoretical foundations for the description of preferences in multiple criteria decision making models using quantitative estimates of the relative importance of criteria. We develop simple decision rules that define the preference and indifference relations induced by the information about the quantitative importance of criteria. These relations are naturally consistent with the values of the generalized, or global, criteria. Copyright © 2002 John Wiley \& Sons, Ltd.},,
year = {2002}
}

